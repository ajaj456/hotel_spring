<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.Booked">

	<!-- 게시판리스트 -->
	<select id="list" resultType="com.hotel.booked.model.Booked">
		select bno, payck, inoutck, r.roomNo, stay, m.id, startDate from booked b, member m, room r 
		order by b.bno desc;
	</select>


	<!-- 게시판 글보기 -->
	<select id="view" resultType="com.hotel.booked.model.Booked">
		
	</select>

	<!-- 게시판 글쓰기 -->
	<insert id="write">
	
	</insert>

	<!-- 게시판 글수정 -->
	<update id="update">
		
	</update>

	<!-- 게시판글삭제 -->
	<delete id="delete">

	</delete>

</mapper>