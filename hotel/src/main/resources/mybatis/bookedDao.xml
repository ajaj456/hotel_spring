<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.Booked">

	<!-- 예약 리스트 -->
	<select id="list" resultType="com.hotel.booked.model.Booked">
		select bno, payck, inoutck, roomNo,
		stay, id, to_char(startDate, 'yyyy') as year, to_char(startDate, 'mm')
		as month, to_char(startDate, 'dd') as day, people from booked
		order by bno desc


	</select>


	<!-- 예약 보기 -->
	<select id="view" resultType="com.hotel.booked.model.Booked">
	</select>

	<!-- 예약 하기 -->
	<insert id="write">

	</insert>

	<!-- 예약 수정 -->
	<update id="update">

	</update>

	<!-- 예약 삭제 -->
	<delete id="delete">

	</delete>

</mapper>